
public class Test {
	public static void main(String[] args){
		Personfinal taro = new Personfinal();
		taro.name="山田太郎";
		taro.age=20;

		Personfinal jiro = new Personfinal();
		jiro.name="木村次郎";
		jiro.age=18;

		Personfinal hanako = new Personfinal();
		hanako.name="鈴木花子";
		hanako.age=16;

		Personfinal takeda = new Personfinal();
		takeda.name="武田勝頼";
		takeda.age=24;

		System.out.println(taro.name);
		System.out.println(taro.age);
		System.out.println(jiro.name);
		System.out.println(jiro.age);
		System.out.println(hanako.name);
		System.out.println(hanako.age);
		System.out.println(takeda.name);
		System.out.println(takeda.age);
	}

}


public class Person {
	protected String name = null;
	protected int age = 0;

	public Person(String name,int age){
		this.name=name;
		this.age=age;
	}

	public String getName(){
		return this.name;
	}
	public void setName(String name){
		this.name=name;
	}
	public void setName(int age){
		this.age=age;
	}
}
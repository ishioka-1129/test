
public class Person {
	private String name;
	private int age = 0;

	public String getName(){
		return this.name;
	}
	public void setName(String name){
		this.name=name;
	}
	public int getAge(){
		return this.age;
	}
	public void setAge(int age){
		this.age=age;
	}
}
private int zero = 0;
private int one = 0;
